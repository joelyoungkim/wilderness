/*
* Link Style
*/
a:link    {color:#384037; background-color:transparent; text-decoration:none}
a:visited {color:#373C40; background-color:transparent; text-decoration:none}
a:hover   {color:#6C6D80; background-color:transparent; text-decoration:none}
a:active  {color:#403B37; background-color:transparent; text-decoration:none}

ul.menu-header li {
	float: left;
	//float: center;
}

ul.menu-header li a {
	display: inline-block;
	text-align: center;
	padding: 10px 5px;
	text-decoration: none;
	width: 70px;
	//width: 75px;
}

ul.menu-header {
  list-style-type: none;
  margin: 0;
  padding: 0;
  overflow: hidden;
  background-color: #E6E6F2;
  font-size: 11pt;
}

ul.product {
  list-style-type: none;
  margin: 0;
  padding: 0;
  overflow: hidden;
}
@media screen and (min-width : 701px){
	ul.product li {
		float: left;
		//float: left;
	}
}

@media screen and (max-width : 700px){
	ul.product li {
		float: center;
	}
}

ul.product li a {
	display: inline-block;
	text-align: center;
	padding: 14px 33px;
	text-decoration: none;
	color: #2a3131;
}

ul.product li a:hover {
    background-color: #fff3e0;
    color: black;
}



